@model List<Generwell.Web.ViewModels.TaskViewModel>




    @if (Model.Count() > 0)
        {
         @foreach (var item in Model)
         {
             @if (item.Status == "Re-activated")
             {
                <button id="SaveTaskFieldDetailsId" type="submit" name="Complete" value="Complete" class="btn button taskdetailsbuttons">Save</button>

             }
             else
             {

                <button id="SaveTaskFieldDetailsId" type="submit" name="Save" value="Save" class="btn button taskdetailsbuttons">Save</button>
             }
         }
        }






